    "test": "jest --verbose",
    "eject": "react-scripts eject",
    "lint": "npx eslint `find . -name '*.ts*' -not -path './node_modules/*'`",
    "lint:fix": "npx eslint `find . -name '*.ts*' -not -path './node_modules/*'` --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jj-stigell/srs-client.git"
  },
